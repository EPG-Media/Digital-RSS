    <style type="text/css">
        /* Client-specific Styles */
        #outlook a {
            padding: 0;
        }

        /* Force Outlook to provide a "view in browser" menu link. */
        body {
            width: 100% !important;
            -webkit-text-size-adjust: 100%;
            -ms-text-size-adjust: 100%;
            Margin: 0;
            padding: 0;
        }

        /* Force Hotmail to display normal line spacing.  More on that: https://www.emailonacid.com/forum/viewthread/43/ */
        #backgroundTable {
            Margin: 0;
            padding: 0;
            width: 100% !important;
            line-height: 100% !important;
            background-color: #e7e7e7;
        }

        /* End reset */
        p {
            Margin: 1em 0;
        }

        img {
            outline: none;
            text-decoration: none;
            -ms-interpolation-mode: bicubic;
            display: block;
            Margin: auto;
        }

        a {
            color: #b40f0f;

        }

        a img {
            border: none;
        }

        /********************************************************************************************

        User Styles

        ********************************************************************************************/

        /** Whisper Text **/
        .whisperText td {
            font: 14px Helvetica, Arial, sans-serif !important;
            padding: 0.3em 0 !important;
        }

        /** Nav Bar Area **/
        .navBar td {
            text-align: center !important;
            padding: 0.5em !important;
            background-color: #000000;
            font: 22px Ubuntu, Arial, sans-serif !important;
        }

        .navBar a {
            font: 22px Ubuntu, Arial, sans-serif !important;
            Margin: auto !important;
            color: #f8f8f8 !important;
            text-decoration: none !important;
        }

        .navBar2 {
            border-left: 3px solid #f8f8f8 !important;
            text-align: center !important;
        }

        .arrowBar td {
            text-align: center !important;
        }

        /** Ribbon Styling **/
        .featureRibbon {
            background-color: #ff0002;
        }

        .featureRibbon td {
            color: #f8f8f8 !important;
            padding: 10px 5px 10px 15px !important;
            font: 26px Ubuntu, Arial, sans-serif !important;
            text-decoration: none !important;
        }
        .featureRibbon a {
            text-decoration: none !important;
        }


        /** Card Styling **/
        .featureContainer {
            Margin-bottom: 20px !important;
        }

        .featureBorder {
            background-color: #ffffff;
            border-bottom: 2px solid #e0e0e0 !important;
            border-left: 1px solid #eaeaea !important;
            border-right: 2px solid #e0e0e0 !important;
        }

        .smallFeature,
        .appStore {
            display: inline-block;
            margin: 15px 12px 20px !important;
            border-top: 1px solid #eaeaea !important;
        }

        .smallFeature td,
        .subscribeText {
            padding: 6px 10px !important;
        }

        /** Text Formats **/
        .smallFeatureCategory {
            color: #676767 !important;
            font: 16px Ubuntu, Arial, sans-serif !important;
        }

        .smallFeatureHeader {
            color: #000000 !important;
            font: bold 20px Ubuntu, Arial, sans-serif !important;
        }

        .featureText {
            color: #000000 !important;
            font: 18px Ubuntu, Arial, sans-serif !important;
            padding: 15px 30px !important;
        }

        .smallFeatureText {
            color: #b40f0f;
            font: 16px Ubuntu, Arial, sans-serif !important;
            padding-bottom: 15px !important;
        }

        .smallFeatureText li {
            font-size: 18px !important;
            Margin-bottom: 20px !important;
        }

        .subscribeHeader {
            color: #b40f0f !important;
            font: bold 28px Ubuntu, Arial, sans-serif !important;
            text-decoration: none !important;
        }

        .subscribeText {
            color: #676767 !important;
            font: bold 20px Ubuntu, Arial, sans-serif !important;
        }

        /** Images **/
        .featureImage {
            Margin: auto !important;
            padding: 15px 10px 15px !important;
            border-bottom: 2px solid #e0e0e0 !important;
            text-align: center !important;
        }

        .socialMediaIcon img {
            Margin: 20px !important;
        }
    </style>

</head>
