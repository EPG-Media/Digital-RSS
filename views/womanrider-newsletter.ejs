  <style type="text/css">
  /* Client-specific Styles */
  #outlook a {
    padding: 0;
  }

  /* Force Outlook to provide a "view in browser" menu link. */
  body {
    width: 100% !important;
    -webkit-text-size-adjust: 100%;
    -ms-text-size-adjust: 100%;
    margin: 0;
    padding: 0;
  }

  /* Force Hotmail to display normal line spacing.  More on that: https://www.emailonacid.com/forum/viewthread/43/ */
  #backgroundTable {
    margin: 0;
    padding: 0;
    width: 100% !important;
    line-height: 100% !important;
    background-color: #e7e6e6;
  }

  /* End reset */
  p {
    margin: 1em 0;
  }

  img {
    outline: none;
    text-decoration: none;
    -ms-interpolation-mode: bicubic;
    display: block;
    margin: auto;
  }

  a {
    color: #002C6B;
  }

  a img {
    border: none;
  }

  /********************************************************************************************

  User Styles

  ********************************************************************************************/

  /** Whisper Text **/
  .whisperText td {
    font: 14px Helvetica, Arial, sans-serif !important;
    padding: 0.3em 0 !important;
  }

  .headerImage{
    width: 52%;
  }

  /** Nav Bar Area **/
  .navBar td {
    text-align: center !important;
    padding: 0.5em !important;
    background-color: #000000;
  }

  .navBar a {
    font: 18px 'Palatino Linotype', Palatino, serif !important;
    margin: auto !important;
    color: #f8f8f8 !important;
    text-decoration: none !important;
  }

  .navBar2 {
    border-left: 3px solid #f8f8f8 !important;
    text-align: center !important;
  }

  .arrowBar td {
    text-align: center !important;
  }

  /** Ribbon Styling **/
  .featureRibbon {
    /* background-color: #ff66cc; */
    background-color: #000000;
  }

  .featureRibbon td {
    color: #f8f8f8 !important;
    padding: 10px 5px 10px 15px !important;
    font: 26px 'Palatino Linotype', Palatino, serif !important;
  }

  /** Card Styling **/
  .featureContainer {
    margin-bottom: 20px !important;
  }

  .featureBorder {
    background-color: #ffffff;
    border-bottom: 2px solid #e0e0e0 !important;
    border-left: 1px solid #eaeaea !important;
    border-right: 2px solid #e0e0e0 !important;
  }

  .smallFeature,
  .appStore {
    display: inline-block;
    margin: 15px 12px 20px !important;
    border-top: 1px solid #eaeaea !important;
  }

  .smallFeature td,
  .subscribeText {
    padding: 6px 10px !important;
  }

  /** Text Formats **/
  .smallFeatureCategory {
    color: #676767 !important;
    font: 16px 'Palatino Linotype', Palatino, serif !important;
  }

  .smallFeatureHeader {
    color: #b40f0f !important;
    font: bold 20px 'Palatino Linotype', Palatino, serif !important;
  }

  .featureText {
    color: #676767 !important;
    font: 18px Helvetica, Arial, sans-serif !important;
    padding: 15px 30px !important;
  }

  .smallFeatureText {
    color: #676767;
    font: 16px Helvetica, Arial, sans-serif !important;
    padding-bottom: 15px !important;
  }

  .smallFeatureText li {
    font-size: 18px !important;
    margin-bottom: 20px !important;
  }

  .subscribeHeader {
    color: #b40f0f !important;
    font: bold 28px 'Palatino Linotype', Palatino, serif !important;
    text-decoration: none !important;
  }

  .subscribeText {
    color: #676767 !important;
    font: bold 20px 'Palatino Linotype', Palatino, serif !important;
  }

  /** Images **/
  .featureImage {
    margin: auto !important;
    padding: 15px 10px 15px !important;
    border-bottom: 2px solid #e0e0e0 !important;
    text-align: center !important;
  }

  .socialMediaIcon img {
    margin: 20px !important;
  }

  </style>
</head>

